# Convert all text files to LF on commit and use LF on checkout
* text=auto eol=lf

# Explicit settings for some common file types
*.java     text eol=lf
*.xml      text eol=lf
*.properties text eol=lf
*.sql      text eol=lf
*.html     text eol=lf
*.js       text eol=lf
*.css      text eol=lf
*.md       text eol=lf
